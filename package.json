{
	"name": "virtual-phone",
	"version": "1.0.0",
	"scripts": {
		"dev": "concurrently --raw \"blitz dev\" 'quirrel'",
		"build": "blitz build",
		"start": "blitz start",
		"studio": "blitz prisma studio",
		"lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
		"test": "jest",
		"test:watch": "jest --watch",
		"prepare": "husky install"
	},
	"engines": {
		"node": "15"
	},
	"prisma": {
		"schema": "db/schema.prisma"
	},
	"prettier": {
		"semi": true,
		"useTabs": true,
		"tabWidth": 4,
		"printWidth": 100,
		"trailingComma": "all",
