		"@tailwindcss/typography": "0.4.1",
		"@twilio/voice-sdk": "2.0.1",
		"awesome-phonenumber": "2.64.0",
		"aws-sdk": "2.1043.0",
		"blitz": "0.44.0",
		"clsx": "1.1.1",
		"got": "11.8.3",
		"jotai": "1.4.6",
		"luxon": "2.1.1",
		"next-pwa": "5.4.1",
		"pino": "6.13.3",
		"pino-pretty": "7.1.0",
