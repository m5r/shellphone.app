		"web-push": "3.4.5",
		"zod": "3.8.2"
	},
	"devDependencies": {
		"@types/luxon": "2.0.4",
		"@types/pino": "6.3.11",
		"@types/preview-email": "2.0.1",
		"@types/react": "17.0.23",
		"@types/test-listen": "1.1.0",
		"@types/web-push": "3.3.2",
		"autoprefixer": "10.3.4",
		"concurrently": "6.2.1",
		"eslint": "7.32.0",
		"husky": "6.0.0",
		"isomorphic-unfetch": "3.1.0",
		"lint-staged": "11.1.2",
